    #contactModal.modal.fade(tabindex="-1", role="dialog", aria-labelledby="myModalLabel", aria-hidden="true", style="")
        .modal-dialog
            .modal-content.contactUs
                span.require
                    span.require-red *
                    | required fields
                .modal-header
                    button.close(type="button", data-dismiss="modal", aria-hidden="true") &#215;
                    h4#myModalLabel.modal-title Contact us
                .modal-body.clearfix
                    #registerModalerror(style="display: none;")
                    form#form-contact(enctype="multipart/form-data", method="post")
                        .row-fluid
                            label(for="message-text") Your message
                                span.require-red *
                            p
                                textarea#message-text.form-control(cols="80", rows="8", name="message")
                            label(for="message-name") Your name
                            p
                                input#message-name.form-control(type="text", name="name", size="20", maxlength="255", value="")
                            label(for="message-email") Your email
                            p
                                input#message-email.form-control(type="text", name="email", size="20", maxlength="255", value="")
                        .modal-footer
                            button#registerModalsubmit.btn(type="submit", name="send_message", style="margin-top: 0;") Submit

